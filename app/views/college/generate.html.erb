<div class="Page-Area">
    <%= link_to(root_path) do %>
    <div class="Back-Link-Area">
      <div class="Back-Link-Icon">
        <span class="material-icons-outlined">
        door_back
        </span>
      </div>
      <div class="Back-Link-Text">
      ホームへ戻る
      </div>
    </div>
    <% end %>
  <div class="Page-Title">
  カレッジを作成
  </div>
  <div class="Page-Text">
  カレッジを作成することができます。
  </div>
  <div class="Dashboard-DoubleGroup-w">
    <div class="Dashboard-Contents">
        <%= form_with(model: @college, url: {controller: 'college', action: 'new'}) do |f| %>

        <div class="form-group">
          <%= f.label "カレッジ名" %>
          <%= f.text_field :name, required: true, class: 'form-control'%>
        </div>

        <div class="form-group">
          <%= f.label "識別コード" %>
          <%= f.text_field :flag, required: true, class: 'form-control'%>
        </div>

      <div class="form-group">
        <%= f.submit "Sign up", class: 'Submit-btn' %>
      </div>
    <% end %>

    </div>
    <div class="Dashboard-Contents">


    </div>
  </div>
</div>